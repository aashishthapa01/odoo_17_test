<!-- my_module/views/report_template.xml -->
<odoo>
    <data>
        <template id="report_custom_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <div class="page">
                        <h2><t t-esc="o.name"/></h2>
                        <p><t t-esc="o.address"/></p>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>
