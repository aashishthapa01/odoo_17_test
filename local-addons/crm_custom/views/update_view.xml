<odoo>
  <data>
    <record id="custom_crm_lead_view_form" model="ir.ui.view">
      <field name="name">custom.crm.lead.view_form</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_lead_view_form" />
      <field name="arch" type="xml">
        <xpath expr="//group[@name='opportunity_partner']/field[@name='partner_id']" position="after">
            <field name='category'></field>
            <field name='industry_id'></field>
        </xpath>
      </field>
    </record>
  </data>
</odoo>